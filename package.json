{
  "name": "levelupcode-header",
  "displayName": "LevelUpCode Header",
  "description": "Insère un header LevelUpCode avec ASCII et pseudo dynamique",
  "version": "0.0.5",
  "publisher": "Nivmizz7",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:levelupcode.insertHeader",
    "onCommand:levelupcode.changeUsername"
  ],
  "main": "extension.js",
  "contributes": {
    "commands": [
      {
        "command": "levelupcode.insertHeader",
        "title": "Insert LevelUpCode Header"
      },
      {
        "command": "levelupcode.changeUsername",
        "title": "Change LevelUpCode Username"
      }
    ],
    "configuration": {
      "properties": {
        "levelupcode.username": {
          "type": "string",
          "default": "",
          "description": "Pseudo à utiliser dans le header"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LevelUpC0de/header.git"
  },
  "dependencies": {}
}
